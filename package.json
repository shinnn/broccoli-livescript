{
  "name": "broccoli-livescript",
  "version": "0.2.0",
  "description": "LiveScript compiler for Broccoli",
  "main": "index.js",
  "scripts": {
    "lint": "jshint *.js test/*.js --reporter node_modules/jshint-stylish/stylish",
    "build": "rimraf test/actual && broccoli build test/actual",
    "pretest": "${npm_package_scripts_lint} & npm run-script build",
    "test": "mocha test/test.js --reporter spec --timeout 300",
    "posttest": "rimraf tmp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinnn/broccoli-livescript.git"
  },
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinnn/broccoli-livescript/issues"
  },
  "homepage": "https://github.com/shinnn/broccoli-livescript",
  "keywords": [
    "broccoli-plugin",
    "livescript",
    "language",
    "compiler",
    "transpiler",
    "syntactic sugar",
    "altjs"
  ],
  "dependencies": {
    "broccoli-filter": "^0.1.6",
    "LiveScript": "^1.2.0"
  },
  "devDependencies": {
    "broccoli": "^0.12.2",
    "jshint": "^2.5.2",
    "jshint-stylish": "^0.4.0",
    "mocha": "^1.21.1",
    "rimraf": "^2.2.8"
  }
}
