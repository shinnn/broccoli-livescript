{
  "name": "broccoli-livescript",
  "version": "1.0.0",
  "description": "LiveScript compiler for Broccoli",
  "repository": "shinnn/broccoli-livescript",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/broccoli-livescript/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "broccoli-plugin",
    "livescript",
    "language",
    "compiler",
    "transpiler",
    "syntactic sugar",
    "altjs"
  ],
  "scripts": {
    "pretest": "jscs *.js test/test.js && eslint *.js test/test.js",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "LiveScript": "^1.3.1",
    "broccoli-filter": "^0.1.6",
    "object-assign": "^2.0.0"
  },
  "devDependencies": {
    "broccoli": "^0.13.3",
    "eslint": "^0.14.1",
    "fs-readfile-promise": "^1.1.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "rimraf": "^2.2.8",
    "tap-spec": "^2.2.1",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
