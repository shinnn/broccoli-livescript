{
  "name": "broccoli-livescript",
  "version": "1.0.0",
  "description": "LiveScript compiler for Broccoli",
  "repository": "shinnn/broccoli-livescript",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/broccoli-livescript/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "broccoli-plugin",
    "livescript",
    "language",
    "compiler",
    "transpiler",
    "syntactic sugar",
    "altjs"
  ],
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc-node/rc.json index.js test/test.js",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "LiveScript": "^1.3.1",
    "broccoli-filter": "^0.1.13",
    "object-assign": "^2.0.0"
  },
  "devDependencies": {
    "@shinnn/eslintrc-node": "^1.0.2",
    "broccoli": "^0.16.2",
    "eslint": "^0.20.0",
    "fs-readfile-promise": "^1.1.0",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "rimraf": "^2.3.3",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  }
}
